e}\n            />\n            </div>\n            <h1 class=\"timeDisplay\">{formatTime(displayTime)}</h1>\n            <button onClick={controlTime} type=\"button\" class=\"btn btn-outline-danger\">\n                Play\n            </button>\n            <button onClick={resetTime} type=\"button\" class=\"btn btn-outline-danger\">\n                Reset\n            </button>\n        </div>\n    )\n}\n\nfunction Length({title, changeTime, type, time, formatTime}) {\n    return (\n        <div>\n            <h3>{title}</h3>\n            <div className=\"time-sets\">\n                <button value=\"-\" onClick={() => changeTime(-60, type)}>\n                    -\n                </button>\n                <h3>{formatTime(time)}</h3>\n                <button value=\"+\" onClick={() => changeTime(+60, type)}>\n                    +\n                </button>\n            </div>\n        </div>\n    )\n}\n\nexport default App;\n",{"ruleId":"15","replacedBy":"16"},{"ruleId":"17","replacedBy":"18"},{"ruleId":"19","severity":1,"message":"20","line":10,"column":24,"nodeType":"21","messageId":"22","endLine":10,"endColumn":37},"no-native-reassign",["23"],"no-negated-in-lhs",["24"],"no-unused-vars","'setBreakAudio' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]